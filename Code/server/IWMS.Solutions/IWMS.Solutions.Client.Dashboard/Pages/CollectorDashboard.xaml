<Window x:Class="IWMS.Solutions.Client.Dashboard.CollectorDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dashboard" Height="352" Width="432" WindowStartupLocation="CenterScreen" WindowState="Maximized" Loaded="Window_Loaded">
    <Grid>
        <DataGrid Name="dgCollectors" AutoGenerateColumns="False" Margin="0,0,0,37">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Name" Binding="{Binding Name}" />
                <DataGridTextColumn Header="Mobile" Binding="{Binding Mobile}" />
                <DataGridTextColumn Header="Address" Binding="{Binding Address}" />
                <DataGridTextColumn Header="Ward" Binding="{Binding Ward}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
